  ],
  "definitions": {
    "alleleOrigins" : {
      "type": "array",
      "description": "Origin of the variant allele",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "allelicRequirements": {
      "type": "array",
      "description": "Inheritance patterns",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "beta": {
      "type": "number",
      "description": "Effect size of numberic traits."
    },
    "biologicalModelAllelicComposition": {
      "type": "string",
      "description": "Allelic composition of the model organism",
      "examples": [
        "Lepr<tm1Jke>/Lepr<tm1Jke>"
      ]
    },
    "biologicalModelGeneticBackground": {
      "type": "string",
      "description": "Genetic background of the model organism",
      "examples": [
        "involves: 129X1/SvJ"
      ]
    },
    "biologicalModelId": {
      "type": "string",
      "description": "Identifier of the biological model (eg. in MGI).",
      "examples": [
        "MGI:5766745"
      ],
      "pattern": "^(MGI:)\\d+$"
    },
    "cellLineBackground": {
      "type": "string",
      "description": "Background of the derived cell lines.",
      "examples": [
        "KOLF2-C1"
      ]
    },
    "cellType": {
      "type": "string",
      "description": "The studied cell type. Preferably the cell line ontology label.",
      "examples": [
        "HaCaT keratinocytes",
        "iPSC derived cortical neurons"
      ]
    },
    "biomarkerName": {
      "type": "string",
      "description": "Altered characteristics that influences the disease process.",
      "examples": [
        "BRAF (V600E,V600D,V600K,V600M,V600G,V600R)"
      ]
    },
    "biomarkers": {
      "type": "object",
      "description": "List of biomarkers.",
      "properties": {
        "variant": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "examples": [
                  "BRAF:V600M"
                ]
              },
              "id": {
                "$ref": "#/definitions/variantId"
              },
              "variantFunctionalConsequenceId": {
                "$ref": "#/definitions/variantFunctionalConsequenceId"
              }
            },
            "required": ["name"],
            "additionalProperties": false
          },
          "uniqueItems": true
        },
        "geneExpression": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "Raw gene expression annotation from the source",
                "examples": [
                  "ANXA1:over"
                ]
              },
              "id": {
                "type": "string",
                "description": "Gene Ontology (GO) identifiers of regulation or background expression processes.",
                "pattern": "^GO_\\d+",
                "examples": [
                  "GO_0010467"
                ]
              }
            },
            "required": ["name"],
            "additionalProperties": false
          },
          "uniqueItems": true
        }
      },
      "additionalProperties": false
    },
    "clinicalPhase": {
      "type": "integer",
      "description": "Phase of the clinical trial",
      "minimum": 0,
      "maximum": 4
    },
    "clinicalSignificances": {
      "type": "array",
      "description": "Standard terms to define clinical significance",
      "items": {
        "type": "string",
        "examples": [
          "pathogenic"
        ],
        "enum": [
          "affects",
          "association",
          "association not found",
          "benign",
          "confers sensitivity",
          "conflicting data from submitters",
          "conflicting interpretations of pathogenicity",
          "drug response",
          "likely benign",
          "likely pathogenic",
          "not provided",
          "other",
          "pathogenic",
          "protective",
          "risk factor",
          "uncertain significance"
        ]
      },
      "uniqueItems": true
    },
    "clinicalStatus": {
      "type": "string",
      "description": "Current stage of a clinical study",
      "examples": [
        "Enrolling by invitation"
      ],
      "enum": [
        "Suspended",
        "Unknown status",
        "Completed",
        "Enrolling by invitation",
        "Not yet recruiting",
        "Recruiting",
        "Terminated",
        "N/A",
        "Withdrawn",
        "Active, not recruiting"
      ]
    },
    "cohortDescription": {
      "type": "string",
      "description": "Description of the studied cohort",
      "examples": [
        "Prostate adenocarcinoma data from SU2C-PCF Dream Team"
      ]
    },
    "cohortId": {
      "type": "string",
      "description": "Identifier of the studied cohort",
      "examples": [
        "CBIOP_WXS_PRAD_SU2C_2019"
      ]
    },
    "cohortShortName": {
      "type": "string",
      "description": "Short name of the studied cohort",
      "examples": [
        "PRAD_SU2C_MET"
      ]
    },
    "cohortPhenotypes": {
      "type": "array",
      "description": "Clinical features/phenotypes observed in studied individuals",
      "items": {
        "type": "string",
        "examples": [
          "Anterior segment dysgenesis 5"
        ]
      },
      "uniqueItems": true
    },
    "confidence": {
      "type": "string",
      "description": "Confidence qualifier on the reported evidence"
    },
    "confidenceIntervalLower": {
      "type": "number",
      "description": "Lower value of the confidence interval"
    },
    "confidenceIntervalUpper": {
      "type": "number",
      "description": "Upper value of the confidence interval"
    },
    "contrast": {
      "type": "string",
      "description": "Experiment contrast",
      "examples": [
        "glioblastoma vs normal"
      ]
    },
    "crisprScreenLibrary": {
      "type": "string",
      "description": "The applied screening library in the CRISPR/CAS9 project.",
      "examples": [
        "Kosuke v1.1 (Behan et al., 2018)"
      ]
    },
    "datasourceId": {
      "type": "string"
    },
    "datatypeId": {
      "type": "string",
      "enum": [
        "affected_pathway",
        "literature",
        "rna_expression",
        "genetic_literature",
        "animal_model",
        "somatic_mutation",
        "known_drug",
        "genetic_association",
        "ot_partner"
      ]
    },
    "direction": {
      "type": "string",
      "description": "Which end of the distribution the target was picked from.",
      "examples": [
        "upper tail",
        "lower tail"
      ]

    },
    "diseaseCellLines": {
      "type": "array",
      "description": "Cancer cell lines used to generate evidence",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "diseaseFromSource": {
      "type": "string",
      "description": "Disease label from the original source",
      "examples": [
        "MYOPATHY, SPHEROID BODY"
      ]
    },
    "diseaseFromSourceId": {
      "type": "string",
      "description": "Disease ID from the original source",
      "examples": [
        "OMIM:182920"
      ]
    },
    "diseaseFromSourceMappedId": {
      "type": "string",
      "description": "Identifier of the disease in the EFO ontology",
      "pattern": "(^NCIT_C\\d+$|^Orphanet_\\d+$|^GO_\\d+$|^HP_\\d+$|^EFO_\\d+$|^MONDO_\\d+$|^DOID_\\d+$|^MP_\\d+$|^OTAR_\\d+$)",
      "examples": [
        "EFO_0005537"
      ]
    },
    "diseaseModelAssociatedHumanPhenotypes": {
      "type": "array",
      "description": "Human phenotypes equivalent to those observed in animal models",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "pattern": "^HP:\\d+$"
          },
          "label": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "uniqueItems": true
    },
    "diseaseModelAssociatedModelPhenotypes": {
      "type": "array",
      "description": "Phenotypes observed in genetically-modified animal models",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "pattern": "^MP:\\d+$",
            "examples": [
              "MP:0002060"
            ]
          },
          "label": {
            "type": "string",
            "examples": [
              "abnormal skin morphology"
            ]
          }
        },
        "additionalProperties": false
      },
      "uniqueItems": true
    },
    "drugFromSource": {
      "type": "string",
      "description": "Drug name/family in resource of origin.",
      "examples": [
        "Imatinib"
      ]
    },
    "drugId": {
      "type": "string",
      "description": "Drug molecule ID",
      "pattern": "^(CHEMBL)\\d+$",
      "examples": [
        "CHEMBL1431",
        "CHEMBL803"
      ]
    },
    "geneticBackground": {
      "type": "string",
      "description": "Additional genetic background of the applied cell line in the CRISPR screen."
    },
    "drugResponse": {
      "type": "string",
      "description": "Patterns of drug response.",
      "examples": [
        "Not Responsive"
      ]
    },
    "literature": {
      "type": "array",
      "description": "PubMed reference identifiers",
      "items": {
        "type": "string",
        "pattern": "\\d+$"
      },
      "uniqueItems": true
    },
    "log2FoldChangePercentileRank": {
      "type": "integer",
      "description": "Percentile of top differentially regulated genes (transcripts) within experiment"
    },
    "log2FoldChangeValue": {
      "type": "number",
      "description": "Log2 fold expression change in contrast experiment"
    },
    "mutatedSamples": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "functionalConsequenceId": {
            "type": "string",
            "description": "Sequence ontology (SO) identifier of the functional consequence of the variant",
            "pattern": "SO_\\d+",
            "examples": [
              "SO_0001628"
            ]
          },
          "numberMutatedSamples": {
            "type": "integer",
            "description": "Number of cohort samples in which target is mutated with a mutation of any type",
            "exclusiveMinimum": 0
          },
          "numberSamplesTested": {
            "type": "integer",
            "description": "Number of cohort samples tested",
            "exclusiveMinimum": 0
          },
          "numberSamplesWithMutationType": {
            "type": "integer",
            "description": "Number of cohort samples in which target is mutated with a specific mutation type",
            "exclusiveMinimum": 0
          }
        },
        "additionalProperties": false
      },
      "uniqueItems": true
    },
    "oddsRatio": {
      "type": "number",
      "description": "Odds ratio (e.g GWAS association)"
    },
    "pathways": {
      "type": "array",
      "description": "List of pathways",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Reactome pathway identifier.",
            "pattern": "^(R-HSA-)\\d+$",
            "examples": [
              "R-HSA-4615885"
            ]
          },
          "name": {
            "type": "string",
            "description": "Pathway or gene set name",
            "examples": [
              "Chaperonin-mediated protein folding",
              "Signaling by FGFR3"
            ]
          }
        },
        "additionalProperties": false
      },
      "uniqueItems": true
    },
    "pmcIds": {
      "type": "array",
      "description": "List of PubMed Central identifiers of full text publication.",
      "items": {
        "type": "string",
        "pattern": "^PMC\\d+$",
        "description": "Literature identifer in PubMed Central"
      },
      "uniqueItems": true
    },
    "projectId":{
      "type": "string",
      "description": "The identifer of the project that generated the data.",
      "examples": [
        "FINNGEN",
        "UKBB",
        "OTAR033"
      ]
    },
    "pValueExponent": {
      "type": "integer",
      "description": "p-value (e.g. GWAS association) exponent",
      "maximum": -8
    },
    "pValueMantissa": {
      "type": "number",
      "description": "p-value (e.g. GWAS association) exponent",
      "minimum": 0.0
    },
    "publicationFirstAuthor": {
      "type": "string",
      "description": "First author of the publication",
      "examples": [
        "Davies G",
        "Kanai M",
        "Pirastu N",
        "Astle WJ",
        "Kichaev G"
      ]
    },
    "publicationYear": {
      "type": "integer",
      "description": "Year of publication",
      "pattern": "\\d{4}$"
    },
    "reactionId": {
      "type": "string",
      "description": "Pathway reaction ID",
      "pattern": "^(R-HSA-)\\d+$",
      "examples": [
        "R-HSA-9660615"
      ]
    },
    "reactionName": {
      "type": "string",
      "description": "Name corresponding to the reaction ID",
      "examples": [
        "Viral Protein Synthesis"
      ]
    },
    "resourceScore": {
      "type": "number",
      "description": "Score provided by datasource indicating strength of target-disease association",
      "minimum": 0.0
    },
    "significantDriverMethods": {
      "type": "array",
      "description": "Methods to detect cancer driver genes producing significant results",
      "items": {
        "type": "string",
        "examples": [
          "mutpanning"
        ],
        "enum": [
          "oncodriveclustl",
          "combination",
          "smregions",
          "mutpanning",
          "cbase",
          "hotmaps",
          "dndscv",
          "oncodrivefml"
        ]
      },
      "uniqueItems": true
    },
    "studyCases": {
      "type": "integer",
      "description": "Number of cases in case-control study",
      "exclusiveMinimum": 0
    },
    "studyId": {
      "type": "string",
      "description": "Identifier of the study"
    },
    "studyOverview": {
      "type": "string",
      "description": "Description of the study"
    },
    "studySampleSize": {
      "type": "integer",
      "description": "Sample size of study",
      "exclusiveMinimum": 0
    },
    "studyStartDate": {
      "type": "string",
      "format": "date",
      "description": "Start date of study in a YYYY-MM-DD format",
      "pattern": "(((19|20)([2468][048]|[13579][26]|0[48])|2000)[-]02[-]29|((19|20)[0-9]{2}[-](0[469]|11)[-](0[1-9]|[12][0-9]|30)|(19|20)[0-9]{2}[-](0[13578]|1[02])[-](0[1-9]|[12][0-9]|3[01])|(19|20)[0-9]{2}[-]02[-](0[1-9]|1[0-9]|2[0-8])))"
    },
    "studyStopReason": {
      "type": "string",
      "description": "Reason why a study has been stopped",
      "pattern": "^[^\\ ].*[^\\ ]$"
    },
    "targetFromSourceId": {
      "type": "string",
      "description": "Target ID in resource of origin (accepted sources include Ensembl gene ID, Uniprot ID, gene symbol)",
      "examples": [
        "BRCA1",
        "ENSG00000012048",
        "P38398"
      ]
    },
    "targetFromSource": {
      "type": "string",
      "description": "Target name/synonym or non HGNC symbol in resource of origin",
      "examples": [
        "3-mercaptopyruvate sulfurtransferase"
      ]
    },
    "targetInModelEnsemblId": {
      "type": "string",
      "description": "Target Ensembl ID in animal model",
      "pattern": "^(ENSMUSG)\\d+$",
      "examples": [
        "ENSMUSG00000014773"
      ]
    },
    "targetInModelMgiId": {
      "type": "string",
      "description": "Target MGI ID in animal model",
      "pattern": "^(MGI:)\\d+$",
      "examples": [
        "MGI:104659"
      ]
    },
    "targetInModel": {
      "type": "string",
      "description": "Target name/synonym in animal model",
      "examples": [
        "Dll1"
      ]
    },
    "targetModulation": {
      "type": "string",
      "description": "Description of target modulation event"
    },
    "textMiningSentences": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "dEnd": {
            "type": "integer",
            "description": "Index position where disease name ends in sentence"
          },
          "dStart": {
            "type": "integer",
            "description": "Index position where disease name starts in sentence"
          },
          "section": {
            "type": "string",
            "description": "Section in which sentence occurs",
            "enum": [
              "title",
              "table",
              "other",
              "figure",
              "appendix",
              "abstract"
            ]
          },
          "tEnd": {
            "type": "integer",
            "description": "Index position where target name ends in sentence"
          },
          "text": {
            "type": "string",
            "description": "Sentence text"
          },
          "tStart": {
            "type": "integer",
            "description": "Index position where target name starts in sentence"
          }
        },
        "additionalProperties": false
      },
      "uniqueItems": true
    },
    "biosamplesFromSource": {
      "type": "array",
      "description": "Identifier of the referenced biological material",
      "items": {
        "type": "string",
        "pattern": "(UBERON_\\d+|BTO_\\d+|EFO_\\d+|CL_\\d+)",
        "examples": [
          "UBERON_0002107",
          "BTO_0001202",
          "CL_0000019",
          "EFO_0000572"
        ]
      },
      "uniqueItems": true
    },
    "urls": {
      "type": "array",
      "description": "Reference to linked external resource (e.g. clinical trials, studies, package inserts, reports, etc.)",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "pattern": "https://|http://",
            "examples": [
              "https://clinicaltrials.gov/search?id=%22NCT01917656%22",
              "https://search.clinicalgenome.org/kb/gene-validity/d910a9d8-516e-443d-acba-8d61f7574792--2018-06-07T14:23:53"

            ]
          },
          "niceName": {
            "type": "string",
            "examples": [
              "Clinical Trials",
              "DailyMed",
              "FDA",
              "ATC"
            ]
          }
        },
        "additionalProperties": false
      },
      "uniqueItems": true
    },
    "variantFunctionalConsequenceId": {
      "type": "string",
      "description": "Sequence ontology (SO) identifier of the functional consequence of the variant",
      "pattern": "SO_\\d+",
      "examples": [
        "SO_0001628"
      ]
    },
    "variantId": {
      "type": "string",
      "description": "Identifier in CHROM_POS_REF_ALT notation of the disease-causing variant.",
      "pattern": "^([0-9XY]{1,2}|MT)_\\d+_[A-Z]+_[A-Z]+$",
      "examples": [
        "20_41203988_T_C",
        "MT_91234_C_W"
      ]
    },
    "variantRsId": {
      "type": "string",
      "description": "Variant reference SNP cluster ID (Rsid)",
      "pattern": "^rs[0-9]{1,}$",
      "examples": [
        "rs539139475"
      ]
    },
    "variantAminoacidDescriptions": {
      "type": "array",
      "description": "Descriptions of variant consequences at protein level",
      "items": {
        "type": "string",
        "examples": [
          "glycine 598 replaced with L-valine",
          "Deletion of residues 550 to 558"
        ]
      },
      "uniqueItems": true
    }
  }
}
