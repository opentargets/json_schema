  "definitions": {
    "allelicRequirements": {
      "type": "array",
      "description": "Inheritance patterns",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "biologicalModelAllelicComposition": {
      "type": "string",
      "description": "Allelic composition of the mouse model",
      "examples": [
        "Lepr<tm1Jke>/Lepr<tm1Jke>"
      ]
    },
    "biologicalModelGeneticBackground": {
      "type": "string",
      "description": "Genetic background of the mouse model",
      "examples": [
        "involves: 129X1/SvJ"
      ]
    },
    "clinicalPhase": {
      "type": "integer",
      "description": "Phase of the clinical trial",
      "minimum": 0,
      "maximum": 4
    },
    "clinicalSignificances": {
      "type": "array",
      "description": "Clinical significance definition",
      "items": {
        "type": "string",
        "examples": [
          "pathogenic"
        ]
      },
      "uniqueItems": true
    },
    "clinicalStatus": {
      "type": "string",
      "description": "Current stage of a clinical study",
      "examples": [
        "Enrolling by invitation"
      ]
    },
    "clinicalUrls": {
      "type": "array",
      "description": "Link to clinical trial information",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "pattern": "https://|http://",
            "examples": [
              "https://clinicaltrials.gov/search?id=%22NCT01917656%22"
            ]
          },
          "niceName": {
            "type": "string",
            "enum": [
              "Clinical Trials Information",
              "DailyMed Information",
              "FDA Information",
              "ATC Information"
            ]
          }
        }
      },
      "uniqueItems": true
    },
    "cohortDescription": {
      "type": "string",
      "description": "Description of the cohort",
      "examples": [
        "Prostate adenocarcinoma data from SU2C-PCF Dream Team"
      ]
    },
    "cohortId": {
      "type": "string",
      "description": "Identifier of the studied cohort",
      "examples": [
        "CBIOP_WXS_PRAD_SU2C_2019"
      ]
    },
    "cohortShortName": {
      "type": "string",
      "description": "Short name of the studied cohort",
      "examples": [
        "PRAD_SU2C_MET"
      ]
    },
    "cohortPhenotypes": {
      "type": "array",
