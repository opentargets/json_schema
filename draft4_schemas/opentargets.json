  "definitions": {
    "allelicRequirements": {
      "type": "array",
      "description": "Inheritance patterns",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "biologicalModelAllelicComposition": {
      "type": "string",
      "description": "Allelic composition of the model organism",
      "examples": [
        "Lepr<tm1Jke>/Lepr<tm1Jke>"
      ]
    },
    "biologicalModelGeneticBackground": {
      "type": "string",
      "description": "Genetic background of the model organism",
      "examples": [
        "involves: 129X1/SvJ"
      ]
    },
    "clinicalPhase": {
      "type": "integer",
      "description": "Phase of the clinical trial",
      "minimum": 0,
      "maximum": 4
    },
    "clinicalSignificances": {
      "type": "array",
      "description": "Standard terms to define clinical significance",
      "items": {
        "type": "string",
        "examples": [
          "pathogenic"
        ],
        "enum": [
          "uncertain significance",
          "pathogenic",
          "likely benign",
          "association not found",
          "other",
          "benign",
          "association",
          "risk factor",
          "likely pathogenic",
          "not provided",
          "affects",
          "protective",
          "drug response",
          "conflicting interpretations of pathogenicity"
        ]
      },
      "uniqueItems": true
    },
    "clinicalStatus": {
      "type": "string",
      "description": "Current stage of a clinical study",
      "examples": [
        "Enrolling by invitation"
      ],
      "enum": [
        "Suspended",
        "Unknown status",
        "Completed",
        "Enrolling by invitation",
        "Not yet recruiting",
        "Recruiting",
        "Terminated",
        "N/A",
        "Withdrawn",
        "Active, not recruiting"
      ]
    },
    "clinicalUrls": {
      "type": "array",
      "description": "Reference to linked clinical information (studies, package inserts, etc.)",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "pattern": "https://|http://",
            "examples": [
              "https://clinicaltrials.gov/search?id=%22NCT01917656%22"
            ]
          },
          "niceName": {
            "type": "string",
            "enum": [
              "Clinical Trials",
              "DailyMed",
              "FDA",
              "ATC"
            ]
          }
        }
      },
      "uniqueItems": true
    },
    "cohortDescription": {
      "type": "string",
      "description": "Description of the studied cohort",
      "examples": [
        "Prostate adenocarcinoma data from SU2C-PCF Dream Team"
      ]
    },
    "cohortId": {
      "type": "string",
      "description": "Identifier of the studied cohort",
      "examples": [
        "CBIOP_WXS_PRAD_SU2C_2019"
      ]
    },
    "cohortShortName": {
      "type": "string",
      "description": "Short name of the studied cohort",
      "examples": [
        "PRAD_SU2C_MET"
      ]
    },
    "cohortPhenotypes": {
      "type": "array",
      "description": "Clinical features/phenotypes observed in studied individuals",
      "items": {
        "type": "string",
        "examples": [
          "Anterior segment dysgenesis 5"
        ]
      },
      "uniqueItems": true
    },
    "confidence": {
      "type": "string",
      "description": "Confidence qualifier on the reported evidence"
    },
    "confidenceIntervalLower": {
      "type": "number",
      "description": "Lower value of the confidence interval"
    },
    "confidenceIntervalUpper": {
      "type": "number",
      "description": "Upper value of the confidence interval"
    },
    "contrast": {
      "type": "string",
      "description": "Experiment contrast",
      "examples": [
        "glioblastoma vs normal"
      ]
    },
    "datasourceId": {
      "type": "string"
    },
    "datatypeId": {
      "type": "string",
      "enum": [
        "affected_pathway",
        "literature",
        "rna_expression",
        "genetic_literature",
        "animal_model",
        "somatic_mutation",
        "known_drug",
        "genetic_association"
      ]
    },
    "diseaseCellLines": {
      "type": "array",
      "description": "Cancer cell lines used to generate evidence",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "diseaseFromSource": {
      "type": "string",
      "description": "Disease label from the original source",
      "examples": [
        "MYOPATHY, SPHEROID BODY"
      ]
    },
    "diseaseFromSourceId": {
      "type": "string",
      "description": "Disease ID from the original source",
      "examples": [
        "OMIM:182920"
      ]
    },
    "diseaseFromSourceMappedId": {
      "type": "string",
      "description": "Identifier of the disease in the EFO ontology",
      "pattern": "(^NCIT_C\\d+$|^Orphanet_\\d+$|^GO_\\d+$|^HP_\\d+$|^EFO_\\d+$|^MONDO_\\d+$|^DOID_\\d+$|^MP_\\d+$)",
      "examples": [
        "EFO_0005537"
      ]
    },
    "diseaseModelAssociatedHumanPhenotypes": {
      "type": "array",
      "description": "Human phenotypes equivalent to those observed in animal models",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "pattern": "^HP:\\d+$"
          },
          "label": {
            "type": "string"
          }
        }
      },
      "uniqueItems": true
    },
    "diseaseModelAssociatedModelPhenotypes": {
      "type": "array",
      "description": "Phenotypes observed in genetically-modified animal models",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "pattern": "^MP:\\d+$",
            "examples": [
              "MP:0002060"
            ]
          },
          "label": {
            "type": "string",
            "examples": [
              "abnormal skin morphology"
            ]
          }
        }
      },
      "uniqueItems": true
    },
    "drugId": {
      "type": "string",
      "description": "Drug molecule ID",
      "pattern": "^(CHEMBL)\\d+$",
      "examples": [
        "CHEMBL1431",
        "CHEMBL803"
      ]
    },
    "literature": {
      "type": "array",
      "description": "Literature reference identifiers (Pubmed or PMC)",
      "items": {
        "type": "string",
        "pattern": "\\d+$"
      },
      "uniqueItems": true
    },
    "log2FoldChangePercentileRank": {
      "type": "integer",
      "description": "Percentile of top differentially regulated genes (transcripts) within experiment"
    },
    "log2FoldChangeValue": {
      "type": "number",
      "description": "Log2 fold expression change in contrast experiment"
    },
    "mutatedSamples": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "functionalConsequenceId": {
            "type": "string",
            "description": "Sequence ontology (SO) identifier of the functional consequence of the variant",
            "pattern": "SO_\\d+",
            "examples": [
              "SO_0001628"
            ]
          },
          "numberMutatedSamples": {
            "type": "integer",
            "description": "Number of cohort samples in which target is mutated with a specific mutation type",
            "exclusiveMinimum": true,
            "minimum": 0
          },
          "numberSamplesTested": {
            "type": "integer",
            "description": "Number of cohort samples tested",
            "exclusiveMinimum": true,
            "minimum": 0
          },
          "numberSamplesWithMutationType": {
            "type": "integer",
            "description": "Number of cohort samples in which target is mutated with a specific mutation type",
            "exclusiveMinimum": true,
            "minimum": 0
          }
        }
      },
      "uniqueItems": true
    },
    "oddsRatio": {
      "type": "number",
      "description": "Odds ratio (e.g GWAS association)"
    },
    "pathwayId": {
      "type": "string",
      "description": "Pathway or gene set ID",
      "pattern": "^(R-HSA-)\\d+$",
      "examples": [
        "R-HSA-4615885"
      ]
    },
    "pValueExponent": {
      "type": "integer",
      "description": "p-value (e.g. GWAS association) exponent",
      "maximum": -8
    },
    "pValueMantissa": {
      "type": "number",
      "description": "p-value (e.g. GWAS association) exponent",
      "minimum": 0.0
    },
    "pathwayName": {
      "type": "string",
      "description": "Pathway or gene set name",
      "examples": [
        "Chaperonin-mediated protein folding",
        "Signaling by FGFR3"
      ]
    },
    "publicationFirstAuthor": {
      "type": "string",
      "description": "First author of the publication",
      "examples": [
        "Davies G",
        "Kanai M",
        "Pirastu N",
        "Astle WJ",
        "Kichaev G"
      ]
    },
    "publicationYear": {
      "type": "integer",
      "description": "Year of publication",
      "pattern": "\\d{4}$"
    },
    "reactionId": {
      "type": "string",
      "description": "Pathway reaction ID",
      "pattern": "^(R-HSA-)\\d+$",
      "examples": [
        "R-HSA-9660615"
      ]
    },
    "resourceScore": {
      "type": "number",
      "description": "Score provided by datasource indicating strength of target-disease association",
      "minimum": 0.0
    },
    "significantDriverMethods": {
      "type": "array",
      "description": "Methods to detect cancer driver genes producing significant results",
      "items": {
        "type": "string",
        "examples": [
          "mutpanning"
        ],
        "enum": [
          "oncodriveclustl",
          "combination",
          "smregions",
          "mutpanning",
          "cbase",
          "hotmaps",
          "dndscv",
          "oncodrivefml"
        ]
      },
      "uniqueItems": true
    },
    "studyCases": {
      "type": "integer",
      "description": "Number of cases in case-control study",
      "exclusiveMinimum": true,
      "minimum": 0
    },
    "studyId": {
      "type": "string",
      "description": "Identifier of the study"
    },
    "studyOverview": {
      "type": "string",
      "description": "Description of the study"
    },
    "studySampleSize": {
      "type": "integer",
      "description": "Sample size of study",
      "exclusiveMinimum": true,
      "minimum": 0
    },
    "targetFromSourceId": {
      "type": "string",
      "description": "Target ID in resource of origin (accepted sources include Ensembl gene ID, Uniprot ID, gene symbol)",
      "examples": [
        "BRCA1",
        "ENSG00000012048",
        "P38398"
      ]
    },
    "targetInModel": {
      "type": "string",
      "description": "Target ID in animal model",
      "pattern": "^(ENSMUSG)\\d+$"
    },
    "targetModulation": {
      "type": "string",
      "description": "Description of target modulation event"
    },
    "textMiningSentences": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "dEnd": {
            "type": "integer",
            "description": "Index position where disease name ends in sentence"
          },
          "dStart": {
            "type": "integer",
            "description": "Index position where disease name starts in sentence"
          },
          "section": {
            "type": "string",
            "description": "Section in which sentence occurs",
            "enum": [
              "title",
              "table",
              "other",
              "figure",
              "appendix",
              "abstract"
            ]
          },
          "tEnd": {
            "type": "integer",
            "description": "Index position where target name ends in sentence"
          },
          "text": {
            "type": "string",
            "description": "Sentence text"
          },
          "tStart": {
            "type": "integer",
            "description": "Index position where target name starts in sentence"
          }
        }
      },
      "uniqueItems": true
    },
    "variantFunctionalConsequenceId": {
      "type": "string",
      "description": "Sequence ontology (SO) identifier of the functional consequence of the variant",
      "pattern": "SO_\\d+",
      "examples": [
        "SO_0001628"
      ]
    },
    "variantId": {
      "type": "string",
      "description": "Identifier of the variant using CHROM_POS_REF_ALT notation",
      "pattern": "^[0-9XY]{1,2}_\\d+_[ACTG]+_[ACTG]+$",
      "examples": [
        "20_41203988_T_C"
      ]
    },
    "variantRsId": {
      "type": "string",
      "description": "Variant reference SNP cluster ID (Rsid)",
      "pattern": "^rs[0-9]{1,}$",
      "examples": [
        "rs539139475"
      ]
    },
    "variantAminoacidDescriptions": {
      "type": "array",
      "description": "Descriptions of variant consequences at protein level",
      "items": {
        "type": "string",
        "examples": [
          "glycine 598 replaced with L-valine",
          "Deletion of residues 550 to 558"
        ]
      },
      "uniqueItems": true
    }
  }
}